<GameObject>
	<Render>
		<Model name="box" objFileName="box/box.obj"/>
	</Render>
	
	<TranslateOverTime
		movingLeft="true"
		xUnitsPerSecond="18.0"
		leftLimit="-100.0"
		rightLimit="100.0"
		
		movingDown="true"
		yUnitsPerSecond="12.0"
		lowerLimit="-100.0"
		upperLimit="100.0"
		
		movingForward="false"
		zUnitsPerSecond="15.0"
		forwardLimit="-200.0"
		backwardLimit="-100.0"
	/>
	
	<RotateOverTime 
		xRadiansPerSecond="0.2" 
		yRadiansPerSecond="0.2" 
		zRadiansPerSecond="0.2"
	/>
	
	<ScaleOverTime
		xScalingUp="true"
		xScalePerSecond="0.8"
		xLowerLimit="10.0"
		xUpperLimit="20.0"
		
		yScalingUp="false"
		yScalePerSecond="0.2"
		yLowerLimit="10.0"
		yUpperLimit="20.0"
		
		zScalingUp="false"
		zScalePerSecond="0.4"
		zLowerLimit="10.0"
		zUpperLimit="20.0"
	/>
</GameObject>