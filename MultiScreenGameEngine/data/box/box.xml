<GameObject>
	<Render>
		<Model name="box" objFileName="box/box.obj"/>
	</Render>
	
	<TranslateOverTime
		movingLeft="true"
		xUnitsPerSecond="60.0"
		leftLimit="150.0"
		rightLimit="650.0"
		
		movingDown="true"
		yUnitsPerSecond="48.0"
		lowerLimit="550.0"
		upperLimit="50.0"
		
		movingForward="false"
		zUnitsPerSecond="90.0"
		forwardLimit="100.0"
		backwardLimit="-300.0"
	/>
	
	<RotateOverTime 
		xRadiansPerSecond="0.2" 
		yRadiansPerSecond="0.2" 
		zRadiansPerSecond="0.2"
	/>
	
	<ScaleOverTime
		xScalingUp="true"
		xScalePerSecond="4.0"
		xLowerLimit="50.0"
		xUpperLimit="150.0"
		
		yScalingUp="false"
		yScalePerSecond="1.0"
		yLowerLimit="50.0"
		yUpperLimit="150.0"
		
		zScalingUp="false"
		zScalePerSecond="8.0"
		zLowerLimit="50.0"
		zUpperLimit="150.0"
	/>
</GameObject>