<GameObject>
	<Render>
		<Model name="box" objFileName="box/box.obj"/>
	</Render>
	
	<TranslateOverTime
		movingLeft="true"
		xUnitsPerSecond="36.0"
		leftLimit="-50.0"
		rightLimit="50.0"
		
		movingDown="true"
		yUnitsPerSecond="50.0"
		lowerLimit="-50.0"
		upperLimit="50.0"
		
		movingForward="false"
		zUnitsPerSecond="30.0"
		forwardLimit="-200.0"
		backwardLimit="-100.0"
	/>
	
	<RotateOverTime 
		xRadiansPerSecond="0.2" 
		yRadiansPerSecond="0.2" 
		zRadiansPerSecond="0.2"
	/>
	
	<ScaleOverTime
		xScalingUp="true"
		xScalePerSecond="1.6"
		xLowerLimit="5.0"
		xUpperLimit="25.0"
		
		yScalingUp="false"
		yScalePerSecond="0.8"
		yLowerLimit="5.0"
		yUpperLimit="25.0"
		
		zScalingUp="false"
		zScalePerSecond="1.2"
		zLowerLimit="5.0"
		zUpperLimit="25.0"
	/>
</GameObject>