<GameObject>
	<RigidBody
		active="true"
		allowSleep="true"
		angularDamping="0.0"
		awake="false"
		bullet="false"
		fixedRotation="true"
		gravityScale="0.0"
		linearDamping="0.0"
		type="static"
	>
		<Fixture 
			density="0.15"
			friction="0.0" 
			isSensor="true" 
			restitution="0.0"
			categoryBits="1"
			maskBits="12"
		>
			<Shape type="box" halfWidth="0.5" halfHeight="0.5" x="0.0" y="0.0"/>
		</Fixture>
		<OnCollideEvents>
			<Event collidedWith="ball" eventType="GOAL_SCORED" ballParameterName="ball"/>
		</OnCollideEvents>
	</RigidBody>
</GameObject>